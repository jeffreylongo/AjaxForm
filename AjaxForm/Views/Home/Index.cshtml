@{
    ViewBag.Title = "Home Page";
}

<div class="wrapper">
    <form class="form-signin">
        <h2 class="form-signin-heading">Lead Entry</h2>
        <input class="form-control" placeholder="First Name" required="" />
        <input class="form-control"  placeholder="Last Name" required="" />
        <input class="form-control" placeholder="Email" required="" />
        <input class="form-control" placeholder="Phone" required="" />
        <div class="form-group">
            <label for="sel1">Mobile Phone #:</label>
            <select class="form-control" id="sel1">
                <option>Yes</option>
                <option>No</option>
            </select>
        </div>
        <label class="checkbox">
            <input type="checkbox" value="optOut" id="optOut" name="optOut"> Opt out of Email
        </label>
        <button class="btn btn-lg btn-primary" type="submit">Add Lead</button>
    </form>
</div>

<hr />

<div class="jumbotron">
    <h2>Hot Leads</h2>
    <ul id="formData"></ul>
</div>

